{
	"name": "Joplin Tab Stash",
	"version": "0.1.0",
	"description": "Stash yout tabs as Vikunja tasks",
	"homepage_url": "https://github.com/jackymancs4/vikunja-tab-stash-webext",
	"manifest_version": 3,
	"minimum_chrome_version": "121",
	"browser_specific_settings": {
		"gecko": {
			"id": "vikunja-tab-stash@jackymancs4",
			"strict_min_version": "109.0"
		}
	},
	"icons": {
		"128": "icon.png"
	},
	"permissions": [
		"tabs",
		"storage",
		"notifications"
	],
	"optional_permissions": [
		"<all_urls>"
	],
	"browser_action": {
		"browser_style": true,
		"default_title": "Stash Window in Joplin"
	},
	"options_ui": {
		"page": "options.html"
	},
	"background": {
		"service_worker": "background.js",
		"type": "module",
		"scripts": [ "background.js" ]
	}
}
